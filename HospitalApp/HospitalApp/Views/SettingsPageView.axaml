<!-- Chart Area -->
<Border Grid.Row="1" Margin="0,15,0,10" Height="320">
    <!-- This would be where we'd place a real chart control -->
    <Grid>
        <!-- Y-axis labels with proper positioning -->
        <TextBlock Text="80%" FontSize="14" Foreground="#888888" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="15,20,0,0"/>
        <TextBlock Text="60%" FontSize="14" Foreground="#888888" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="15,100,0,0"/>
        <TextBlock Text="40%" FontSize="14" Foreground="#888888" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="15,180,0,0"/>
        <TextBlock Text="20%" FontSize="14" Foreground="#888888" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="15,260,0,0"/>
        
        <Canvas Margin="50,20,20,30">
            <!-- Light gray horizontal lines -->
            <Line StartPoint="0,0" EndPoint="580,0" Stroke="#DDDDDD" StrokeThickness="1.5"/>
