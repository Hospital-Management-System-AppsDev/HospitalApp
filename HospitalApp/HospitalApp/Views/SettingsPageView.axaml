<!-- Calendar -->
<Border Grid.Column="1" Background="White" CornerRadius="10">
    <Grid Margin="15">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        
        <!-- Calendar Title -->
        <TextBlock Grid.Row="0" Text="January 2025" Foreground="#000000" FontWeight="SemiBold" FontSize="16" 
                   HorizontalAlignment="Center" Margin="0,5,0,15"/> 
        
        <!-- Calendar Navigation -->
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" Content="&lt;" Margin="5,0" FontWeight="Light" Background="Transparent" Foreground="#888888"/>
            <Button Grid.Column="2" Content="&gt;" Margin="5,0" FontWeight="Light" Background="Transparent" Foreground="#888888"/>
        </Grid>
        
        <!-- Calendar Grid -->
        <Grid Grid.Row="2" Margin="0,20,0,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            
            <!-- Day Headers -->
            <Grid Grid.Row="0" Margin="0,0,0,10">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                
                <TextBlock Grid.Column="0" Text="SUN" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
                <TextBlock Grid.Column="1" Text="MON" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
                <TextBlock Grid.Column="2" Text="TUE" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
                <TextBlock Grid.Column="3" Text="WED" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
                <TextBlock Grid.Column="4" Text="THU" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
                <TextBlock Grid.Column="5" Text="FRI" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
                <TextBlock Grid.Column="6" Text="SAT" FontSize="10" HorizontalAlignment="Center" Foreground="#AAAAAA"/>
            </Grid>
            
            <!-- Week 1 -->
            <Grid Grid.Row="1" Margin="0,5,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                
                <TextBlock Grid.Column="0" Text="" FontSize="16" HorizontalAlignment="Center"/>
                <TextBlock Grid.Column="1" Text="1" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="2" Text="2" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="3" Text="3" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="4" Text="4" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="5" Text="5" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="6" Text="6" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
            </Grid>
            
            <!-- Week 2 -->
            <Grid Grid.Row="2" Margin="0,5,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                
                <TextBlock Grid.Column="0" Text="7" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="1" Text="8" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="2" Text="9" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="3" Text="10" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="4" Text="11" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="5" Text="12" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="6" Text="13" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
            </Grid>
            
            <!-- Week 3 -->
            <Grid Grid.Row="3" Margin="0,5,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                
                <TextBlock Grid.Column="0" Text="14" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="1" Text="15" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="2" Text="16" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="3" Text="17" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                <TextBlock Grid.Column="4" Text="18" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
                
                <!-- Selected date with orange circle -->
                <Grid Grid.Column="5">
                    <Ellipse Width="36" Height="36" Fill="#FF5722" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <TextBlock Text="19" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                </Grid>
                
                <TextBlock Grid.Column="6" Text="20" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555555"/>
